<div class="container mt-5">

  <table class="table">
    <thead>
      <tr>
        <th>Cantidad</th>
        <th>Título</th>
        <th>Categoría</th>
        <th>Descripción</th>
        <th>Precio</th>
        <th>Código</th>
      </tr>
    </thead>
    <tbody>
      {{#each products}}
        <tr>
          <td>{{this.quantity}}</td>
          <td>{{this.product.title}}</td>
          <td>{{this.product.category}}</td>
          <td>{{this.product.description}}</td>
          <td>{{this.product.price}}</td>
          <td>{{this.product.code}}</td>
        </tr>
      {{/each}}
    </tbody>
  </table>

</div>